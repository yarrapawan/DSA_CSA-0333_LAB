#include <stdio.h>

int main() {
    int a[100], n, x, i, found = 0;

    printf("Y.Pawan 192424294");

    printf("Enter number of elements: ");
    scanf("%d", &n);

    for (i = 0; i < n; i++) {
        scanf("%d", &a[i]);
    }

    printf("Enter element to search: ");
    scanf("%d", &x);

    for (i = 0; i < n; i++) {
        if (a[i] == x) {
            printf("Element found at index %d\n", i);
            found = 1;
        }
    }

    if (found == 0) {
        printf("Element not found\n");
    }

    return 0;
}
